{
  "kind": "collectionType",
  "collectionName": "schools",
  "info": {
    "singularName": "school",
    "pluralName": "schools",
    "displayName": "Schools",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "deepl": {
          "translate": "translate"
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": false
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "deepl": {
          "translate": "translate"
        }
      }
    },
    "isPublic": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": false
    },
    "acceptingStudents": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": false
    },
    "schoolFee": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "applicationFee": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "startDate": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "endDate": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "students": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "schools",
      "pluginOptions": {
        "deepl": {
          "translate": "translate"
        }
      }
    },
    "staff": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "pluginOptions": {
        "deepl": {
          "translate": "translate"
        }
      }
    },
    "applications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::school-application.school-application",
      "mappedBy": "school",
      "pluginOptions": {
        "deepl": {
          "translate": "translate"
        }
      }
    },
    "applicationQuestions": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::question-collection.question-collection",
      "pluginOptions": {
        "deepl": {
          "translate": "translate"
        }
      }
    },
    "preApplicationText": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "deepl": {
          "translate": "translate"
        }
      }
    },
    "referenceQuestions": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::question-collection.question-collection",
      "pluginOptions": {
        "deepl": {
          "translate": "translate"
        }
      }
    },
    "secondarySchool": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "contactEmail": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "email"
    },
    "stripeAppFeeId": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer"
    },
    "stripeSchoolFeeId": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer"
    },
    "detailedDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        },
        "deepl": {
          "translate": "translate"
        }
      },
      "type": "richtext"
    },
    "currency": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "EUR",
        "GBP",
        "USD"
      ],
      "default": "USD"
    },
    "referencesNeeded": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "default": 2,
      "required": true,
      "min": 0,
      "max": 10
    }
  }
}
