{
  "kind": "collectionType",
  "collectionName": "schools",
  "info": {
    "singularName": "school",
    "pluralName": "schools",
    "displayName": "Schools",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": false
    },
    "description": {
      "type": "text"
    },
    "isPublic": {
      "type": "boolean"
    },
    "acceptingStudents": {
      "type": "boolean"
    },
    "schoolFee": {
      "type": "string"
    },
    "applicationFee": {
      "type": "string"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "students": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "schools"
    },
    "staff": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "applications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::school-application.school-application",
      "mappedBy": "school"
    },
    "applicationQuestions": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::question-collection.question-collection"
    },
    "preApplicationText": {
      "type": "richtext"
    },
    "referenceQuestions": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::question-collection.question-collection"
    },
    "secondarySchool": {
      "type": "boolean",
      "default": false
    }
  }
}
